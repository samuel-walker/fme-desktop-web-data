# -------------------------------------------------------------------------
# -------------------------------------------------------------------------
# Use the TwitterFactory to hit the twitter server.  Then use the
# JSONQueryFactory to explode the response into separate tweets, and extract
# some parts of each tweet.
FACTORY_DEF * TwitterFactory    FACTORY_NAME TweetStreamer_Tweeter    INPUT  FEATURE_TYPE Creator_CREATED       ACTION STREAM       NAMED_CONNECTION "test<space>Twitter"       INCLUDE_GEOMETRY No       LANGUAGES en    OUTPUT TWEET FEATURE_TYPE "TweetStreamer_TwitterResponse"
# -------------------------------------------------------------------------
# For each status update, extract several bits of info... text, time, etc.
FACTORY_DEF * JSONQueryFactory    FACTORY_NAME TweetStreamer_StatusExtractor    INPUT FEATURE_TYPE TweetStreamer_TwitterResponse    MODE EXTRACT    JSON_ATTR _tweet_search_result    QUERIES _tweet_status "json<openbracket><quote>text<quote><closebracket>" _tweet_created_at "json<openbracket><quote>created_at<quote><closebracket>" _tweet_id "json<openbracket><quote>id_str<quote><closebracket>" _tweet_geo "json<openbracket><quote>coordinates<quote><closebracket>" _tweet_username "json<openbracket><quote>user<quote><closebracket><openbracket><quote>screen_name<quote><closebracket>"    OUTPUT EVALUATED FEATURE_TYPE TweetStreamer_CheckForTweetId
#-------------------------------------------------------------------------
# Check to see if the message was a tweet, or something else.
FACTORY_DEF * TestFactory    FACTORY_NAME TweetStreamer_TweetTestFactory    INPUT  FEATURE_TYPE TweetStreamer_CheckForTweetId    TEST @Value(_tweet_id) = null    TEST @Value(_tweet_id) = {}    TEST @Value(_tweet_id) = ""    BOOLEAN_OPERATOR OR    OUTPUT FAILED FEATURE_TYPE "TweetStreamer_CheckForGeometry"
#-------------------------------------------------------------------------
# Determine if the _tweet_geo attribute contains any useful data.  If not
# it will be 'null' or '{}' or an empty string.  If it has geometry, use
# the JSONGeometry function to extract it to the feature.
FACTORY_DEF * TestFactory    FACTORY_NAME TweetStreamer_GeoTestFactory    INPUT  FEATURE_TYPE TweetStreamer_CheckForGeometry    TEST @Value(_tweet_geo) = null    TEST @Value(_tweet_geo) = {}    TEST @Value(_tweet_geo) = ""    BOOLEAN_OPERATOR OR    OUTPUT PASSED FEATURE_TYPE "TweetStreamer_Completed"    OUTPUT FAILED FEATURE_TYPE "TweetStreamer_Completed"       @JSONGeometry(FROM_ATTRIBUTE,GEOJSON,_tweet_geo)       @CoordSys(LL84)
# -------------------------------------------------------------------------
# Collect all the features which are finished processing, and remove all
# unnecessary attributes.  Also, convert the created date to the FME
# date/time format. (This requires removing the timezone information
# from the date/time first.)
Tcl2 proc TweetStreamer_timezone_remover {} {    set dateTimeVal [FME_GetAttribute _tweet_created_at];    set length [string length $dateTimeVal];    if {$length > 0} {       FME_SetAttribute _tweet_created_at [regsub {(\+|-)\d{4}} $dateTimeVal {}];    } }
Tcl2 proc TweetStreamer_date_formatter {} {    catch {       set date [FME_GetAttribute _tweet_created_at];       set seconds [clock scan $date];       FME_SetAttribute _tweet_created_at [clock format $seconds -format {%Y%m%d%H%M%S}];    } }
FACTORY_DEF * TextEncoderFactory    FACTORY_NAME TweetStreamer_OutputFactory    INPUT FEATURE_TYPE TweetStreamer_Completed    ACTION HTML_DECODE    ATTR_TO_ENCODE _tweet_status    RESULT_ATTR _tweet_status    OUTPUT OUTPUT FEATURE_TYPE TweetStreamer_TWEETS       @Tcl2(TweetStreamer_timezone_remover)       @Tcl2(TweetStreamer_date_formatter)       @RemoveAttributes(_tweet_geo)
# -------------------------------------------------------------------------
FACTORY_DEF * JSONQueryFactory    FACTORY_NAME JSONFlattener    INPUT  FEATURE_TYPE TweetStreamer_TWEETS    MODE FLATTEN    JSON_DOCUMENT "@EvaluateExpression(FDIV,STRING_ENCODED,<at>Value<openparen>_tweet_search_result<closeparen>,JSONFlattener)"    READ_FROM_FILE NO    FILEPATH_OR_URL <Unused>    EXPLODE_QUERY "json"    RECURSIVE_FLATTEN YES    ERROR_LIST_ATTR "_json_error"    OUTPUT EXPLODED FEATURE_TYPE JSONFlattener_OUTPUT    OUTPUT REJECTED FEATURE_TYPE JSONFlattener_<REJECTED>
